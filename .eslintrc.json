{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/no-unescaped-entities": ["error", {
      "forbid": [
        {
          "char": "'",
          "alternatives": ["&apos;"]
        },
        {
          "char": "\"",
          "alternatives": ["&quot;"]
        }
      ]
    }],
    "@typescript-eslint/no-unused-vars": "error"
  }
} 